act
  insertBev_Euro, insertBev_Dollar,
  cancelBev,
  sugar, no_sugar,
  coffee_0, coffee_1,
  tea_0, tea_1,
  cappuccino_0, cappuccino_1,
  pour_sugar_0, pour_sugar_1, pour_sugar_2,
  pour_milk_0, pour_milk_1,
  pour_coffee_0, pour_coffee_1, pour_coffee_2,
  pour_tea,
  ringBev, skip_0,
  take_cup;


proc
  Bev_S0 = insertBev_Euro.Bev_S1 + insertBev_Dollar.Bev_S1;
  Bev_S1 = cancelBev.Bev_S14 + sugar.Bev_S2 + no_sugar.Bev_S3;

  Bev_S2 = coffee_0.Bev_S6 + tea_0.Bev_S5 + cappuccino_0.Bev_S4;
  Bev_S3 = cappuccino_1.Bev_S9 + tea_1.Bev_S8 + coffee_1.Bev_S7;

  Bev_S4 = pour_sugar_2.Bev_S9;
  Bev_S5 = pour_sugar_1.Bev_S8;
  Bev_S6 = pour_sugar_0.Bev_S7;

  Bev_S7 = pour_coffee_1.Bev_S12;
  Bev_S8 = pour_tea.Bev_S12;
  Bev_S9 = pour_milk_0.Bev_S11 + pour_coffee_0.Bev_S10;

  Bev_S10 = pour_milk_1.Bev_S12;
  Bev_S11 = pour_coffee_2.Bev_S12;

  Bev_S12 = ringBev.Bev_S13 + skip_0.Bev_S13;
  Bev_S13 = take_cup.Bev_S15;

  Bev_S14 = delta;
  Bev_S15 = delta; 

init
  Bev_S0;

