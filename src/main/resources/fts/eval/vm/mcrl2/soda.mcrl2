act
  pay, free, change,
  cancel, return,
  tea, soda,
  serveTea, serveSoda,
  open, take_0, take_1, close;

proc
  S1 = pay.S2 + free.S3;
  S2 = change.S3;
  S3 = cancel.S4 + tea.S6 + soda.S5;
  S4 = return.S10;
  S5 = serveSoda.S7;
  S6 = serveTea.S7;
  S7 = open.S8 + take_0.S10;
  S8 = take_1.S9;
  S9 = close.S10;
  S10 = delta; 

init
  S1;
